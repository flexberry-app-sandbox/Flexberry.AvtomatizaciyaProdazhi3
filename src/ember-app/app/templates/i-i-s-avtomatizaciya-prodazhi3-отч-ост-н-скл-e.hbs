<h3 class="ui header">{{t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.дата-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.дата
      class=(if (v-get validationModel "дата" "isInvalid") "error")
      data-test-i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e-дата=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "дата" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.склады-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.склады
      displayAttributeName="склад"
      autocomplete=true
      relationName="склады"
      projection="СкладыL"
      title=(t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.склады-caption")
      readonly=readonly
      componentName="складыLookup"
      class=(if (v-get validationModel "склады" "isInvalid") "error")
      data-test-i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e-склады=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "склады" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.номенк-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.номенк
      displayAttributeName="номен"
      autocomplete=true
      relationName="номенк"
      projection="НоменкL"
      title=(t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.номенк-caption")
      readonly=readonly
      componentName="номенкLookup"
      class=(if (v-get validationModel "номенк" "isInvalid") "error")
      data-test-i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e-номенк=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номенк" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.колл
      class=(if (v-get validationModel "колл" "isInvalid") "error")
      label=(t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.колл-caption")
      data-test-i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e-колл=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "колл" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.колВРез-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.колВРез
      displayAttributeName="количество"
      autocomplete=true
      relationName="колВРез"
      projection="КолВРезL"
      title=(t "forms.i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e.колВРез-caption")
      readonly=readonly
      componentName="колВРезLookup"
      class=(if (v-get validationModel "колВРез" "isInvalid") "error")
      data-test-i-i-s-avtomatizaciya-prodazhi3-отч-ост-н-скл-e-колВРез=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "колВРез" "message")}}
  </div>
</form>
